try:
  print(x) #x is not defined so error will print
except NameError:
  print("Variable x is not defined")
except:
  print("Something else went wrong") 
  
  #example2
  
  try: #will print and state nothing went wrong
  print("Hello")
except:
  print("Something went wrong")
else:
  print("Nothing went wrong") 
  
  #example3
  
  try: #x is not defined so wont print
  print(x)
except:
  print("Something went wrong")
finally:
  print("The 'try except' is finished") #will print regardless to show end of program
